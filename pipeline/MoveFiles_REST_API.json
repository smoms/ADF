{
	"name": "MoveFiles_REST_API",
	"properties": {
		"description": "Move files from one folder to another folder",
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "RestResource_BOE_PRO",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DataExtraction_Parquet",
						"type": "DatasetReference",
						"parameters": {
							"folder": "AzureBLOB"
						}
					}
				]
			}
		],
		"parameters": {
			"SourceStore_Location": {
				"type": "String",
				"defaultValue": "tst-alpiq-datalake-bi"
			},
			"SourceStore_Directory": {
				"type": "String",
				"defaultValue": "ORACLE/MX09/MX_DATAMART/TST_DATA_TYPES/2020-06-22 20-38-46"
			},
			"DestinationStore_Location": {
				"type": "String",
				"defaultValue": "destination"
			},
			"DestinationStore_Directory": {
				"type": "String",
				"defaultValue": "2020/06/20/13"
			}
		},
		"folder": {
			"name": "ERSExtractionJobTesting/MoveFile"
		},
		"annotations": []
	}
}